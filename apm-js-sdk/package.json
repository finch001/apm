{
  "name": "apm-js-sdk",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "lerna bootstrap -- --ignore-engines",
    "build": "lerna run build",
    "start": "cd ./packages/apm && npm run start",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release:lerna": "lerna publish --force-publish=* --exact --skip-temp-tag",
    "release": "npm run release:lerna && npm run changelog",
    "format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write && git status",
    "esdoc": "lerna run esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stbui/apm.git"
  },
  "author": "stbui",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stbui/apm/issues"
  },
  "homepage": "https://github.com/stbui/apm#readme",
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.23",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "lerna": "^3.5.1",
    "prettier": "^1.18.2",
    "rollup": "^1.20.1",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.23.0",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.12"
  }
}
